{
	"build_type": "unreal",
	"generate_build_scripts": true,
	"launch": {
		"client": {
			"windows": {
				"command": "../../build/assembly/worker/UnrealClient@Windows/UnrealClient@Windows.exe",
				"arguments": ["+assetDatabaseStrategy", "Local", "+localAssetDatabasePath", "${IMPROBABLE_PROJECT_ROOT}/build/assembly", "+appName", "${IMPROBABLE_PROJECT_NAME}"]
			}
		},
		"fsim": {
			"windows": {
				"command": "../../build/assembly/worker/UnrealFsim@Windows.zip/UnrealFsim@Windows.exe",
				"arguments": ["+assetDatabaseStrategy", "Local", "+localAssetDatabasePath", "${IMPROBABLE_PROJECT_ROOT}/build/assembly", "+appName", "${IMPROBABLE_PROJECT_NAME}"]
			}
		}
  	}
}
